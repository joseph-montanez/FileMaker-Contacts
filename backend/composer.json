{
    "name": "joseph-montanez/contacts",
    "description": "FileMaker Contacts Demo",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Joseph Montanez",
            "email": "jm@shabb.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "laravel/homestead": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/",
            "libs/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
